{
  "name": "boxcar",
  "version": "0.0.1",
  "description": "A choo view for handling lists of data",
  "main": "boxcar.js",
  "scripts": {
    "dep": "dependency-check . && dependency-check . --unused --no-dev",
    "start": "bankai start demo.js --debug",
    "test": "standard && npm run dep && browserify test/*.spec.js | tape-run"
  },
  "browserify": {
    "transform": [
      "yo-yoify",
      "sheetify/transform",
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com:toddself/boxcar.git"
  },
  "keywords": [
    "slickgrid",
    "table",
    "sortable",
    "choo",
    "grid"
  ],
  "author": "Todd Kennedy <todd@selfassembled.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/toddself/boxcar/issues"
  },
  "homepage": "https://github.com/toddself/boxcar#readme",
  "dependencies": {
    "babel-preset-env": "^1.5.2",
    "babelify": "^7.3.0",
    "sheetify": "^6.1.0",
    "yo-yoify": "^3.7.3"
  },
  "devDependencies": {
    "bankai": "^8.1.1",
    "browserify": "^14.4.0",
    "browserify-istanbul": "github:toddself/browserify-istanbul#strip-base-path",
    "dependency-check": "^2.9.1",
    "istanbul": "^0.4.5",
    "opener": "^1.4.3",
    "standard": "^10.0.2",
    "tape": "^4.6.3",
    "tape-istanbul": "^1.1.1",
    "tape-run": "^3.0.0"
  }
}
